{
  "id": "com.swapnil.hotfolder",
  "name": "Hotfolder Auto-Open (Auto-Assume)",
  "version": "1.3.0",
  "main": "index.html",
  "host": {
    "app": "PS",
    "minVersion": "25.12.2"
  },
  "manifestVersion": 5,
  "entrypoints": [
    {
      "type": "panel",
      "id": "hotfolder-panel",
      "label": "Hotfolder Auto-Open",
      "main": "index.html",
      "minimumSize": {
        "width": 280,
        "height": 240
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 320,
        "height": 480
      },
      "preferredFloatingSize": {
        "width": 320,
        "height": 480
      },
      "icons": [
        {
          "width": 32,
          "height": 32,
          "path": "icon@1x.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "dark",
            "light"
          ],
          "species": [
            "generic"
          ]
        }
      ]
    }
  ],
  "requiredPermissions": {
    "localFileSystem": "fullAccess"
  },
  "icons": [
    {
      "width": 48,
      "height": 48,
      "path": "icon@1x.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "dark",
        "light"
      ]
    }
  ]
}